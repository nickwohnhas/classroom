<h2>Student</h2>
<p>Sign up or <%= link_to "Log in", new_session_path %></p>

<%= form_for @user, url: "/student_login" do |f| %>
<%= f.label :name %>:
<%= f.text_field :name %>
<br>
<%= f.label :email %>:
<%= f.text_field :email %>
<br>
<%= f.label :password %>:
<%= f.password_field :password %>
<br>
<%= f.submit "Sign Up" %>
<% end %>
